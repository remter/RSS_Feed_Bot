{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "warn",
        "no-underscore-dangle": "off",
        "no-empty-function": "warn",
        "no-param-reassign": "warn"
    }
}
